{
  "name": "modempay/modempay-php",
  "description": "ModemPay PHP SDK",
  "type": "library",
  "require": {
    "php": "^8.1",
    "symfony/http-client": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "suggest": {
    "illuminate/support": "Required for Laravel integration (^10.0|^11.0)"
  },
  "autoload": {
    "psr-4": {
      "ModemPay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ModemPay\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ModemPay\\Laravel\\ModemPayServiceProvider"
      ],
      "aliases": {
        "ModemPay": "ModemPay\\Laravel\\Facades\\ModemPay"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
